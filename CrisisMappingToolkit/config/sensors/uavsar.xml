<?xml version="1.0"?>
<sensor>
    <name>UAVSAR</name>
    <bands>
        <band>
            <source><name>b1</name></source>
            <name>hh</name>
            <distribution name="water">
                <model>dip</model>
                <mode><range>
                    <minimum>3.0</minimum>
                    <maximum>3.6</maximum>
                </range></mode>
            </distribution>
        </band>
        <band>
            <source><name>b2</name></source>
            <name>hv</name>
            <distribution name="water">
                <model>dip</model>
                <mode><range>
                    <minimum>3.0</minimum>
                    <maximum>3.6</maximum>
                </range></mode>
            </distribution>
        </band>
        <band>
            <source><name>b3</name></source>
            <name>vv</name>
            <distribution name="water">
                <model>dip</model>
                <mode><range>
                    <minimum>3.0</minimum>
                    <maximum>4.1</maximum>
                </range></mode>
            </distribution>
        </band>
        <mask>
            <source><name>b4</name></source>
        </mask>
    </bands>
    <range>
        <minimum>257</minimum>
        <maximum>65000</maximum>
    </range>
    <scaling type="log10" />
</sensor>

