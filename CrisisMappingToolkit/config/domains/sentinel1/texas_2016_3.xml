<?xml version="1.0"?>
<domain>
    <name>Texas, March 2016 Flood</name>
    <sensors>
    
      <sensor>
        <name>NED13</name>
          <range>
              <minimum>0</minimum>
              <maximum>50</maximum>
          </range>
      </sensor>
    
      <sensor>
        <name>MODIS</name>
        <bands>
          <source>
            <start_date>2016-3-28</start_date>
            <end_date>2016-3-29</end_date>
          </source>
        </bands>
      </sensor>

      <sensor>
        <name>Sentinel1</name>
        <bands>
          <source>
            <start_date>2016-3-28</start_date>
            <end_date>2016-3-29</end_date>
          </source>
        </bands>
      </sensor>


    </sensors>
    
    <!-- TODO: Adjust this! -->
    <bbox>
        <bottomleft>
            <lon>-92.7819551</lon>
            <lat>31.3512932</lat>
        </bottomleft>
        <topright>
            <lon>-92.4794924</lon>
            <lat>31.5017611</lat>
        </topright>
    </bbox>
   
    <algorithm_params>
      <water_detect_radar_channel>vv</water_detect_radar_channel>
    </algorithm_params> 
   
    
</domain>

